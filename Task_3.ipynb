   "source": [
    "# write code that prompts the user to enter an integer, and displays whether that number is odd or even.\n",
    "\n",
    "mark=int (input(\"Enter an integer\"))\n",
    "if mark % 2 == 0:\n",
    "    print(\"even\")\n",
    "\n",
    "else:\n",
    "    print(\"odd\") \n"
   ]
